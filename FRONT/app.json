{
  "expo": {
    "name": "Unocculto",
    "slug": "Unocculto",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/logo_white.png",
    "scheme": "unocculto",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.antoinecd.Unocculto"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.antoinecd.Unocculto"
    },
    "web": {
      "output": "static",
      "favicon": "./app/assets/images/logo_white.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./app/assets/images/logo_white.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser",
      [
        "@maplibre/maplibre-react-native",
        {
          "android": {
            "v11": true
          },
          "ios": {
            "v11": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5ec7051b-5cd3-48b5-9537-ded44356176b"
      }
    }
  }
}